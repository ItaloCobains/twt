<div class="col-7">
  <div class="mb-3">
    <%= link_to "Home", root_path, class: "fs-4 text-decoration-none text-black fw-semibold" %>
  </div>
  <div class="mb-3">
    <%= render 'shared/tweet_form' %>
  </div>
  <%= render Tweet.order(created_at: :desc) %>
</div>
<div class="col">
  <h2>Users</h2>
  <ul>
    <% User.find_each do |user| %>
      <li><%= user.username %></li>
    <% end %>
  </ul>
</div>
